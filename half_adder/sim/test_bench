#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5e4e785a97b0 .scope module, "test_bench" "test_bench" 2 2;
 .timescale -9 -12;
v0x5e4e785cb6f0_0 .var "a", 0 0;
v0x5e4e785cb7b0_0 .var "b", 0 0;
v0x5e4e785cb880_0 .net "carry", 0 0, L_0x5e4e785cbbb0;  1 drivers
v0x5e4e785cb980_0 .net "sum", 0 0, L_0x5e4e785cba50;  1 drivers
S_0x5e4e785a9940 .scope function.vec4.s2, "calc_expected" "calc_expected" 2 52, 2 52 0, S_0x5e4e785a97b0;
 .timescale -9 -12;
v0x5e4e785a9b20_0 .var "a_val", 0 0;
v0x5e4e785cac50_0 .var "b_val", 0 0;
; Variable calc_expected is vec4 return value of scope S_0x5e4e785a9940
TD_test_bench.calc_expected ;
    %load/vec4 v0x5e4e785a9b20_0;
    %pad/u 2;
    %load/vec4 v0x5e4e785cac50_0;
    %pad/u 2;
    %add;
    %ret/vec4 0, 0, 2;  Assign to calc_expected (store_vec4_to_lval)
    %end;
S_0x5e4e785cadd0 .scope task, "check_result" "check_result" 2 58, 2 58 0, S_0x5e4e785a97b0;
 .timescale -9 -12;
v0x5e4e785cafd0_0 .var "expected_result", 1 0;
TD_test_bench.check_result ;
    %vpi_call 2 61 "$display", "At time: %t, a = 1'b%b, 1'b%b", $time, v0x5e4e785cb6f0_0, v0x5e4e785cb7b0_0 {0 0 0};
    %load/vec4 v0x5e4e785cb980_0;
    %load/vec4 v0x5e4e785cafd0_0;
    %parti/s 1, 0, 2;
    %cmp/e;
    %flag_get/vec4 4;
    %jmp/0 T_1.2, 4;
    %load/vec4 v0x5e4e785cb880_0;
    %load/vec4 v0x5e4e785cafd0_0;
    %parti/s 1, 1, 2;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_1.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %vpi_call 2 63 "$display", "-------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 64 "$display", "PASSED: Expected sum: 1'b%b, Got: 1'b%b, Expected carry: 1'b%b, Got: 1'b%b", &PV<v0x5e4e785cafd0_0, 0, 1>, v0x5e4e785cb980_0, &PV<v0x5e4e785cafd0_0, 1, 1>, v0x5e4e785cb880_0 {0 0 0};
    %vpi_call 2 65 "$display", "-------------------------------------------------------------------------------------------" {0 0 0};
    %jmp T_1.1;
T_1.0 ;
    %vpi_call 2 67 "$display", "-------------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 68 "$display", "FAILED: Expected sum: 1'b%b, Got: 1'b%b, Expected carry: 1'b%b, Got: 1'b%b", &PV<v0x5e4e785cafd0_0, 0, 1>, v0x5e4e785cb980_0, &PV<v0x5e4e785cafd0_0, 1, 1>, v0x5e4e785cb880_0 {0 0 0};
    %vpi_call 2 69 "$display", "-------------------------------------------------------------------------------------------" {0 0 0};
T_1.1 ;
    %end;
S_0x5e4e785cb0b0 .scope module, "dut" "half_adder" 2 8, 3 1 0, S_0x5e4e785a97b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "sum";
    .port_info 3 /OUTPUT 1 "carry";
L_0x5e4e785cba50 .functor XOR 1, v0x5e4e785cb6f0_0, v0x5e4e785cb7b0_0, C4<0>, C4<0>;
L_0x5e4e785cbbb0 .functor AND 1, v0x5e4e785cb6f0_0, v0x5e4e785cb7b0_0, C4<1>, C4<1>;
v0x5e4e785cb330_0 .net "a", 0 0, v0x5e4e785cb6f0_0;  1 drivers
v0x5e4e785cb3f0_0 .net "b", 0 0, v0x5e4e785cb7b0_0;  1 drivers
v0x5e4e785cb4b0_0 .net "carry", 0 0, L_0x5e4e785cbbb0;  alias, 1 drivers
v0x5e4e785cb580_0 .net "sum", 0 0, L_0x5e4e785cba50;  alias, 1 drivers
    .scope S_0x5e4e785a97b0;
T_2 ;
    %vpi_call 2 16 "$dumpfile", "test_bench.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5e4e785a97b0 {0 0 0};
    %vpi_call 2 19 "$display", "----------------------------------------------------------------------------------------" {0 0 0};
    %vpi_call 2 20 "$display", "---------------------------TESTBECNH FOR HALF ADDER ------------------------------------" {0 0 0};
    %vpi_call 2 21 "$display", "----------------------------------------------------------------------------------------" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e4e785cb6f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e4e785cb7b0_0, 0, 1;
    %delay 1000, 0;
    %load/vec4 v0x5e4e785cb6f0_0;
    %load/vec4 v0x5e4e785cb7b0_0;
    %store/vec4 v0x5e4e785cac50_0, 0, 1;
    %store/vec4 v0x5e4e785a9b20_0, 0, 1;
    %callf/vec4 TD_test_bench.calc_expected, S_0x5e4e785a9940;
    %store/vec4 v0x5e4e785cafd0_0, 0, 2;
    %fork TD_test_bench.check_result, S_0x5e4e785cadd0;
    %join;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e4e785cb6f0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e4e785cb7b0_0, 0, 1;
    %delay 1000, 0;
    %load/vec4 v0x5e4e785cb6f0_0;
    %load/vec4 v0x5e4e785cb7b0_0;
    %store/vec4 v0x5e4e785cac50_0, 0, 1;
    %store/vec4 v0x5e4e785a9b20_0, 0, 1;
    %callf/vec4 TD_test_bench.calc_expected, S_0x5e4e785a9940;
    %store/vec4 v0x5e4e785cafd0_0, 0, 2;
    %fork TD_test_bench.check_result, S_0x5e4e785cadd0;
    %join;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e4e785cb6f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e4e785cb7b0_0, 0, 1;
    %delay 1000, 0;
    %load/vec4 v0x5e4e785cb6f0_0;
    %load/vec4 v0x5e4e785cb7b0_0;
    %store/vec4 v0x5e4e785cac50_0, 0, 1;
    %store/vec4 v0x5e4e785a9b20_0, 0, 1;
    %callf/vec4 TD_test_bench.calc_expected, S_0x5e4e785a9940;
    %store/vec4 v0x5e4e785cafd0_0, 0, 2;
    %fork TD_test_bench.check_result, S_0x5e4e785cadd0;
    %join;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e4e785cb6f0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e4e785cb7b0_0, 0, 1;
    %delay 1000, 0;
    %load/vec4 v0x5e4e785cb6f0_0;
    %load/vec4 v0x5e4e785cb7b0_0;
    %store/vec4 v0x5e4e785cac50_0, 0, 1;
    %store/vec4 v0x5e4e785a9b20_0, 0, 1;
    %callf/vec4 TD_test_bench.calc_expected, S_0x5e4e785a9940;
    %store/vec4 v0x5e4e785cafd0_0, 0, 2;
    %fork TD_test_bench.check_result, S_0x5e4e785cadd0;
    %join;
    %delay 10000, 0;
    %delay 100000, 0;
    %vpi_call 2 48 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
